{
	"compilerOptions": {
	  "outDir": "./dist",	// Ensures build files go to frontend/dist
	  "target": "ES2020",
	  "lib": ["ES2020", "DOM"],
	  "module": "ESNext",
	  "moduleResolution": "Node",
	  "strict": true,
	  "baseUrl": "./src",
	  "paths": {
		"*": ["node_modules/*"]
	  },
	  "esModuleInterop": true
	}
}
  